{
  "functions": {
    "api/apify-proxy.ts": {
      "maxDuration": 300
    },
    "api/youtube-video.ts": {
      "maxDuration": 10
    },
    "api/youtube-channel.ts": {
      "maxDuration": 30
    },
    "api/redirect.ts": {
      "maxDuration": 10
    },
    "api/cron-refresh-videos.ts": {
      "maxDuration": 300
    },
    "api/process-tracking-job.ts": {
      "maxDuration": 300
    },
    "api/trigger-tracking-processor.ts": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron-refresh-videos",
      "schedule": "0 */12 * * *"
    },
    {
      "path": "/api/trigger-tracking-processor",
      "schedule": "* * * * *"
    }
  ],
  "routes": [
    {
      "src": "/l/([^/]+)",
      "dest": "/api/redirect?shortCode=$1"
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
