{
  "functions": {
    "api/apify-proxy.ts": {
      "maxDuration": 300
    },
    "api/youtube-video.ts": {
      "maxDuration": 10
    },
    "api/youtube-channel.ts": {
      "maxDuration": 30
    },
    "api/redirect.ts": {
      "maxDuration": 10
    },
    "api/cron-refresh-videos.ts": {
      "maxDuration": 300
    },
    "api/cron-sync-accounts.ts": {
      "maxDuration": 300
    },
    "api/sync-single-account.ts": {
      "maxDuration": 300
    },
    "api/cron-process-videos.ts": {
      "maxDuration": 300
    },
    "api/process-single-video.ts": {
      "maxDuration": 300
    },
    "api/cron-refresh-reels.ts": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cron-refresh-videos",
      "schedule": "0 */12 * * *"
    },
    {
      "path": "/api/cron-refresh-reels",
      "schedule": "0 */6 * * *"
    }
  ],
  "routes": [
    {
      "src": "/l/([^/]+)",
      "dest": "/api/redirect?shortCode=$1"
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
