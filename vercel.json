{
  "functions": {
    "api/apify-proxy.ts": {
      "maxDuration": 300
    },
    "api/youtube-video.ts": {
      "maxDuration": 10
    },
    "api/youtube-channel.ts": {
      "maxDuration": 30
    },
    "api/redirect.ts": {
      "maxDuration": 10
    },
    "api/cron-refresh-videos.ts": {
      "maxDuration": 300
    },
    "api/cron-sync-accounts.ts": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cron-sync-accounts",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron-refresh-videos",
      "schedule": "0 */12 * * *"
    }
  ],
  "routes": [
    {
      "src": "/l/([^/]+)",
      "dest": "/api/redirect?shortCode=$1"
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
