{
  "functions": {
    "api/apify-proxy.ts": {
      "maxDuration": 300
    },
    "api/redirect.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/l/:shortCode",
      "destination": "/api/redirect?shortCode=:shortCode"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
